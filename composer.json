{
    "name": "shanginn/wtf_happend_bot",
    "description": "Bot that will summarize the chat messages for you",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Nikolai Shangin",
            "email": "shanginn@gmail.com"
        }
    ],
    "require": {
        "shanginn/openai-sdk": "dev-master",
        "phenogram/framework": "^5.1",
        "vlucas/phpdotenv": "^5.6",
        "cycle/orm": "^2.10",
        "ext-pcntl": "*",
        "cycle/migrations": "^4.2",
        "cycle/schema-migrations-generator": "^2.3",
        "cycle/annotated": "^4.2",
        "symfony/var-dumper": "^7.2"
    },
    "minimum-stability": "stable",
    "scripts": {
        "fix": "PHP_CS_FIXER_IGNORE_ENV=1 ./tools/vendor/bin/php-cs-fixer fix src --diff -vvv --config=.php-cs-fixer.php"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "fakerphp/faker": "^1.24",
        "mockery/mockery": "^1.6"
    }
}
